{
  "name": "slidev-theme-prismatic",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev",
    "prismatic",
    "colorful",
    "cards"
  ],
  "author": "Nicolas Fondacci",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.6.0"
  },
  "devDependencies": {
    "@slidev/cli": "^51.6.0",
    "unocss": "66.1.0-beta.13"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "transition": "slide-up",
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}