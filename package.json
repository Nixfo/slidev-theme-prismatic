{
  "name": "slidev-theme-prismatic",
  "version": "0.0.0",
  "type": "module",
  "description": "A colorful, card-based theme for Slidev.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nixfo/slidev-theme-prismatic"
  },
  "bugs": "https://github.com/Nixfo/slidev-theme-prismatic/issues",
  "keywords": [
    "slidev-theme",
    "slidev",
    "prismatic",
    "colorful",
    "cards"
  ],
  "author": "Nicolas Fondacci",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "devDependencies": {
    "@slidev/cli": "^51.6.0",
    "@slidev/types": "^51.6.0",
    "@unocss/preset-legacy-compat": "^66.1.1",
    "unocss": "66.1.0-beta.13"
  },
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "transition": "fade-gradient",
      "fonts": {
        "sans": "Nunito Sans",
        "mono": "Fira Code"
      }
    }
  }
}